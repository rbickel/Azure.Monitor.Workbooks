                    "formatter": 5
                  }
                ],
                "labelSettings": [
                  {
                    "columnId": "_ResourceId",
                    "label": "LogicApp"
                  },
                  {
                    "columnId": "WorkflowName",
                    "label": "Workflow"
                  },
                  {
                    "columnId": "Trend",
                    "label": "Execution trend"
                  },
                  {
